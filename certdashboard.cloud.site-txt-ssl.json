{
	"providerId": "certdashboard.cloud",
	"providerName": "CertDashboard",
	"serviceId": "site-txt-ssl",
	"serviceName": "CertDashboard Site TXT records",
	"version": 1,
	"logoUrl": "https://res.cloudinary.com/dy94cdmsp/image/upload/v1754392054/svgviewer-output_yinzzk.svg",
	"description": "Enable domain to work with the CertDashboard by configuring the necessary CNAME and TXT SSL records.",
	"variableDescription": "txtCdValue: cert dashboard txt value.",
	"syncBlock": false,
	"syncPubKeyDomain": "certdashboard.cloud",
	"syncRedirectDomain": "certdashboard.cloud",
	"records": [
		{
			"type": "TXT",
			"host": "@",
			"data": "%txtCdValue%",
			"ttl": 600
		}
	]
}
