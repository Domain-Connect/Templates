{
    "providerId": "zybr.ai",
    "providerName": "ZYBR",
    "serviceId": "mailgun-autosecurity",
    "serviceName": "Mailgun (Automatic Sender Security)",
    "version": 1,
    "logoUrl": "https://storage.googleapis.com/zybr-cdn/public/static/images/logo_cropped.png",
    "description": "Automatically configures SPF, DKIM via Mailgun Automatic Sender Security (CNAME delegation) and DMARC for sending email through the ZYBR platform.",
    "variableDescription": "%pdk1Target% and %pdk2Target% are the CNAME targets for DKIM delegation provided by Mailgun; %dmarcValue% is the full DMARC policy TXT record.",
    "syncPubKeyDomain": "app.zybr.ai",
    "syncRedirectDomain": "app.zybr.ai,zybr.ai",
    "records": [
        {
            "groupId": "spf",
            "type": "SPFM",
            "host": "@",
            "spfRules": "include:mailgun.org"
        },
        {
            "groupId": "dkim",
            "type": "CNAME",
            "host": "pdk1._domainkey",
            "pointsTo": "%pdk1Target%",
            "ttl": 3600
        },
        {
            "groupId": "dkim",
            "type": "CNAME",
            "host": "pdk2._domainkey",
            "pointsTo": "%pdk2Target%",
            "ttl": 3600
        },
        {
            "groupId": "dmarc",
            "type": "TXT",
            "host": "_dmarc",
            "data": "%dmarcValue%",
            "ttl": 3600
        }
    ]
}
