{
  "providerId": "coopalliance.org",
  "providerName": "Cooperative Computing Alliance",
  "serviceId": "custom-domain",
  "serviceName": "CCA Custom Domain Verification",
  "syncPubKeyDomain": "portal.coopalliance.org",
  "syncRedirectDomain": "portal.coopalliance.org",
  "version": 2,
  "logoUrl": "https://logo.coopalliance.org/logo-black-transparent.png",
  "description": "Configure DNS records to verify ownership of your custom domain for CCA services",
  "variableDescription": "verificationToken - Domain ownership verification token generated by CCA",
  "hostRequired": false,
  "syncBlock": false,
  "records": [
    {
      "type": "TXT",
      "host": "@",
      "data": "%verificationToken%",
      "ttl": 3600
    }
  ]
}
