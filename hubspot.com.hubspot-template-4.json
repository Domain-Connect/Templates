{
  "providerId": "hubspot.com",
  "providerName": "HubSpot",
  "serviceId": "hubspot-template-4-txt",
  "serviceName": "HubSpot CMS Site",
  "version": 1,
  "syncBlock": true,
  "logoUrl": "https://domainconnect.org/wp-content/uploads/2017/02/HubSpot-768x223.png",
  "description": "Enables a domain to work with HubSpot",
  "variableDescription": "txtData: txt data, aDestination: destination for A record, cnameDestination: destination for ",
  "records": [
    {
      "groupId": "txt",
      "type": "TXT",
      "host": "@",
      "data": "%txtData%",
      "ttl": 600
    },
    {
      "groupId": "a",
      "type": "A",
      "host": "@",
      "pointsTo": "%aDestination%",
      "ttl": 600
    },
    {
      "groupId": "cname",
      "type": "CNAME",
      "host": "@",
      "pointsTo": "%cnameDestination%",
      "ttl": 600
    }
  ]
}
