{
  "providerId": "lindo.ai",
  "providerName": "Lindo",
  "serviceId": "whitelabel",
  "serviceName": "Lindo Whitelabel (Wildcard Subdomain)",
  "version": 1,
  "logoUrl": "https://cdn.ln-cdn.com/image/whitelabel/logo/app.lindo.ai.png",
  "description": "Connect your branded wildcard subdomain to Lindo",
  "syncBlock": false,
  "shared": false,
  "syncPubKeyDomain": "lindo.ai",
  "syncRedirectDomain": "app.lindo.ai",
  "hostRequired": true,
  "records": [
    {
      "type": "CNAME",
      "host": "*",
      "pointsTo": "%prefix%.ln-dns.com",
      "ttl": "300"
    },
    {
      "type": "CNAME",
      "host": "_acme-challenge",
      "pointsTo": "%acme%.ssl.ln-dns.com",
      "ttl": "300"
    }
  ]
}
