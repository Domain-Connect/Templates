{
  "providerId":"google.com",
  "providerName":"Google",
  "serviceId":"gsuite-setup",
  "serviceName":"Google GSuite Setup",
  "version": 1,
  "logoUrl":"",
  "description":"Enables a domain to work with Google GSuite",
  "variableDescription":"v1 is the IP address of the server.",
  "multiInstance": true,
  "records":[
    {
      "groupId": "verification",
      "type": "TXT",
      "host": "@",
      "data": "%verifytxt%",
      "ttl": 3600
    },
    {
      "groupId": "setup",
      "type": "TXT",
      "host": "@",
      "data": "v=spf1 %spfrule% -all",
      "ttl": 3600
    }
  ]
}
