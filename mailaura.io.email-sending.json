{
  "providerId": "mailaura.io",
  "providerName": "Mailaura",
  "serviceId": "email-sending",
  "serviceName": "Email Sending Authentication",
  "version": 1,
  "logoUrl": "https://mailaura.io/logo.svg",
  "syncPubKeyDomain": "mailaura.io",
  "syncRedirectDomain": "mailaura.io",
  "warnPhishing": true,
  "description": "Configure DNS records for email sending via Mailaura. Sets up DKIM, SPF, DMARC, and MAIL FROM records for AWS SES.",
  "variableDescription": "dkim1Selector: DKIM selector 1 (from AWS SES); dkim1Value: DKIM CNAME value 1; dkim2Selector: DKIM selector 2 (from AWS SES); dkim2Value: DKIM CNAME value 2; dkim3Selector: DKIM selector 3 (from AWS SES); dkim3Value: DKIM CNAME value 3; mailFromSubdomain: Mail FROM subdomain (default: bounce); dmarcValue: DMARC policy record",
  "records": [
    {
      "groupId": "dkim",
      "type": "CNAME",
      "host": "%dkim1Selector%._domainkey",
      "pointsTo": "%dkim1Value%",
      "ttl": 3600
    },
    {
      "groupId": "dkim",
      "type": "CNAME",
      "host": "%dkim2Selector%._domainkey",
      "pointsTo": "%dkim2Value%",
      "ttl": 3600
    },
    {
      "groupId": "dkim",
      "type": "CNAME",
      "host": "%dkim3Selector%._domainkey",
      "pointsTo": "%dkim3Value%",
      "ttl": 3600
    },
    {
      "groupId": "outbound",
      "type": "MX",
      "host": "%mailFromSubdomain%",
      "pointsTo": "feedback-smtp.eu-central-1.amazonses.com",
      "priority": 10,
      "ttl": 3600
    },
    {
      "groupId": "outbound",
      "type": "SPFM",
      "host": "%mailFromSubdomain%",
      "spfRules": "include:amazonses.com",
      "ttl": 3600
    },
    {
      "groupId": "dmarc",
      "type": "TXT",
      "host": "_dmarc",
      "data": "%dmarcValue%",
      "ttl": 3600
    }
  ]
}
