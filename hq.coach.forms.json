{
    "providerId": "hq.coach",
    "providerName": "HQ Coach",
    "serviceId": "forms",
    "serviceName": "HQ Coach Forms",
    "version": 1,
    "logoUrl": "https://hq.coach/logo.svg",
    "description": "Connect your custom subdomain to HQ Coach for client forms, check-ins, and assessments. Your forms will be served directly on your subdomain (e.g., forms.yourdomain.com).",
    "variableDescription": "%host%: Your subdomain prefix (e.g., 'forms', 'checkin', 'intake')",
    "hostRequired": true,
    "multiInstance": true,
    "records": [
        {
            "type": "CNAME",
            "host": "%host%",
            "pointsTo": "proxy.hq.coach",
            "ttl": 3600
        }
    ]
}
