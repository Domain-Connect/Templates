{
    "providerId": "zeabur.com",
    "providerName": "Zeabur",
    "serviceId": "email",
    "serviceName": "Z-Send Email Service",
    "version": 1,
    "logoUrl": "https://zeabur.com/docs/_next/static/media/logo_b.5eb843fb.svg",
    "description": "Configure email authentication for your domain using Zeabur Z-Send. Sets up DKIM for domain verification, SPF and MX for sending, and DMARC policy.",
    "variableDescription": "dkim1Selector: DKIM selector 1; dkim1Value: DKIM CNAME target 1; dkim2Selector: DKIM selector 2; dkim2Value: DKIM CNAME target 2; dkim3Selector: DKIM selector 3; dkim3Value: DKIM CNAME target 3; mailFromSubdomain: Mail FROM subdomain (e.g., send); region: AWS region (e.g., us-east-1)",
    "warnPhishing": true,
    "records": [
        {
            "groupId": "dkim",
            "type": "CNAME",
            "host": "%dkim1Selector%._domainkey",
            "pointsTo": "%dkim1Value%",
            "ttl": 3600
        },
        {
            "groupId": "dkim",
            "type": "CNAME",
            "host": "%dkim2Selector%._domainkey",
            "pointsTo": "%dkim2Value%",
            "ttl": 3600
        },
        {
            "groupId": "dkim",
            "type": "CNAME",
            "host": "%dkim3Selector%._domainkey",
            "pointsTo": "%dkim3Value%",
            "ttl": 3600
        },
        {
            "groupId": "send",
            "type": "MX",
            "host": "%mailFromSubdomain%",
            "pointsTo": "feedback-smtp.%region%.amazonses.com",
            "priority": "10",
            "ttl": 3600
        },
        {
            "groupId": "send",
            "type": "TXT",
            "host": "%mailFromSubdomain%",
            "data": "v=spf1 include:amazonses.com ~all",
            "ttl": 3600
        },
        {
            "groupId": "dmarc",
            "type": "TXT",
            "host": "_dmarc",
            "data": "v=DMARC1; p=none;",
            "ttl": 3600
        }
    ]
}
