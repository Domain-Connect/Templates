{
    "providerId": "godaddy.com",
    "providerName": "GoDaddy",
    "serviceId": "proemail",
    "serviceName": "Professional Email",
    "version": 1,
    "logoUrl": "https://domainconnect.org/wp-content/uploads/2016/06/godaddy-logo-300x79.png",
    "description": "Enables a domain to work with GoDaddy Professional Email",
    "variableDescription": "Self explanatory in the variable names.",
    "records": [
        {
          "groupId": "Login",
          "type": "CNAME",
          "host": "email",
          "pointsTo": "%email%",
          "ttl": "3600"
        },
        {
          "groupId": "Delivery",
          "type": "MX",
          "host": "@",
          "pointsTo": "%mx%",
          "priority": "0",
          "ttl": "3600"
        },
        {
          "groupId": "Delivery",
          "type": "MX",
          "host": "@",
          "pointsTo": "%mxtwo%",
          "priority": "10",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_autodiscover",
          "protocol": "_tcp",
          "port": "443",
          "weight": "0",
          "priority": "100",
          "name": "@",
          "target": "%autodiscover%",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_submission",
          "protocol": "_tcp",
          "port": "465",
          "weight": "0",
          "priority": "100",
          "name": "@",
          "target": "%submissionhost%",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_imaps",
          "protocol": "_tcp",
          "port": "993",
          "weight": "1",
          "priority": "100",
          "name": "@",
          "target": "%imapshost%",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_imap",
          "protocol": "_tcp",
          "port": "143",
          "weight": "2",
          "priority": "100",
          "name": "@",
          "target": "%imaphost%",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_pop3s",
          "protocol": "_tcp",
          "port": "995",
          "weight": "3",
          "priority": "100",
          "name": "@",
          "target": "%pop3shost%",
          "ttl": "3600"
        },
        {
          "groupId": "Configuration",
          "type": "SRV",
          "service": "_pop3",
          "protocol": "_tcp",
          "port": "110",
          "weight": "4",
          "priority": "100",
          "name": "@",
          "target": "%pop3host%",
          "ttl": "3600"
        },
        {
          "groupId": "Verification",
          "type": "TXT",
          "host": "@",
          "data": "%verifytxt%",
          "ttl": "3600"
        },
        {
          "groupId": "Verification",
          "type": "TXT",
          "host": "@",
          "data": "%spftxt%",
          "ttl": "3600"
        },
    ]
}
