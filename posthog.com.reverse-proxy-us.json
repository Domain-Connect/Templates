{
    "providerId": "posthog.com",
    "providerName": "PostHog",
    "serviceId": "reverse-proxy-us",
    "serviceName": "PostHog Managed Reverse Proxy (US)",
    "version": 1,
    "syncBlock": false,
    "logoUrl": "https://posthog.com/brand/posthog-logo.svg",
    "description": "Configures a CNAME record for PostHog managed reverse proxy",
    "variableDescription": "subdomain: subdomain prefix for the proxy. target: CNAME target generated by PostHog.",
    "syncPubKeyDomain": "domainconnect.posthog.com",
    "syncRedirectDomain": "us.posthog.com",
    "records": [{ "type": "CNAME", "host": "%subdomain%", "pointsTo": "%target%", "ttl": 3600 }]
}
