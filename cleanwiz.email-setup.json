{
  "providerId": "cleanwiz",
  "providerName": "CleanWiz",
  "serviceId": "email-setup",
  "serviceName": "Email Setup",
  "version": 1,
  "logoUrl": "https://app.cleanwiz.ai/logo.png",
  "description": "Automatically configure DNS records for CleanWiz email notifications.",
  "variableDescription": "Your domain configuration parameters",
  "records": [
    {
      "type": "TXT",
      "host": "%dkimSelector%._domainkey",
      "data": "%dkimValue%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "%rpPrefix%",
      "pointsTo": "pm.mtasv.net",
      "ttl": 3600
    }
  ]
}
