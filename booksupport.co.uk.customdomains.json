{
  "providerId": "booksupport.co.uk",
  "providerName": "BookSupport Platform",
  "serviceId": "customdomains",
  "serviceName": "BookSupport Custom Domains",
  "version": 1,
  "logoUrl": "https://raw.githubusercontent.com/Booksupport/BrandAssets/refs/heads/main/Booksupportlogotransparent.svg",
  "description": "Automates the DNS CNAME required to map a BookSupport business to a custom domain.",
  "syncBlock": false,
  "syncPubKeyDomain": "domainconnect.booksupport.co.uk",
  "hostRequired": true,
  "records": [
    {
      "type": "CNAME",
      "host": "@",
      "pointsTo": "customdomains.booksupport.co.uk",
      "ttl": 3600
    }
  ]
}

