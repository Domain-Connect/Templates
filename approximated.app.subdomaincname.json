{  
    "providerId":"approximated.app",
    "providerName":"Approximated",
    "serviceId":"subdomaincname",
    "serviceName":"Subdomain CNAME",
    "version": 1,
    "syncPubKeyDomain" : "domainconnect.approximated.app",
    "description":"Connect a subdomain by a CNAME record to a dynamic domain.",
    "variableDescription":"`targetDomain` refers to the domain the CNAME should point to, provided by the user.",
    "hostRequired": true,
    "records":[  
       {  
          "type":"CNAME",
          "host":"@",
          "pointsTo":"%targetDomain%",
          "ttl":3600
       }
    ]
 }
