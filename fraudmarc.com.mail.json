{
  "providerId": "fraudmarc.com",
  "providerName": "Fraudmarc",
  "serviceId": "mail",
  "serviceName": "Fraudmarc Email ID Protection",
  "logoUrl": "http://domainconnect.org/wp-content/uploads/2017/08/fraudmarc-logo-onLight-300x55.png",
  "description": "Enables Fraudmarc security and ESP Authentication Flow on a domain",
  "variableDescription": "Self explanatory in the variable names.",
  "records": [
    {
      "groupId": "Dkim",
      "type": "NS",
      "host": "_domainkey",
      "pointsTo": "%dkim%",
      "ttl": "3600"
    },
    {
      "groupId": "Dmarc",
      "type": "CNAME",
      "host": "_dmarc",
      "pointsTo": "%dmarc%",
      "ttl": "3600"
    },
    {
      "groupId": "Sts",
      "type": "CNAME",
      "host": "_mta-sts",
      "pointsTo": "%sts1%",
      "ttl": "3600"
    },
    {
      "groupId": "Sts",
      "type": "CNAME",
      "host": "mta-sts",
      "pointsTo": "%sts2%",
      "ttl": "3600"
    },
    {
      "groupId": "Tlsrpt",
      "type": "CNAME",
      "host": "_smtp-tlsrpt",
      "pointsTo": "%tlsrpt%",
      "ttl": "3600"
    },
    {
      "groupId": "Spf",
      "type": "TXT",
      "host": "@",
      "data": "%spf%",
      "ttl": "3600"
    },
    {
      "groupId": "Fm",
      "type": "NS",
      "host": "fraudmarc",
      "pointsTo": "%fm%",
      "ttl": "3600"
    }
  ]
}
