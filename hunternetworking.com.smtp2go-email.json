{
  "providerId": "hunternetworking.com",
  "providerName": "Hunter Networking",
  "serviceId": "smtp2go-email",
  "serviceName": "Email Domain Authentication (SMTP2GO)",
  "version": 1,
  "logoUrl": "https://app.hunternetworking.com/images/logo.png",
  "syncPubKeyDomain": "hunternetworking.com",
  "syncRedirectDomain": "hunternetworking.com",
  "records": [
    {
      "type": "CNAME",
      "host": "%RPATH_SELECTOR%",
      "pointsTo": "%RPATH_TARGET%",
