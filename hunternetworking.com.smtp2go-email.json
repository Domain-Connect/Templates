      "host": "%DKIM_SELECTOR%._domainkey",
      "pointsTo": "%DKIM_TARGET%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "%TRACKING_SUBDOMAIN%",
      "pointsTo": "%TRACKING_TARGET%",
      "ttl": 3600
    }
  ]
}
