{
  "providerId": "goentri.com",
  "providerName": "Entri",
  "serviceId": "mixo",
  "serviceName": "Mixo",
  "version": 5,
  "logoUrl": "https://cdn.goentri.com/logo.svg",
  "description": "Allows user to easily set up domain using Entri",
  "syncPubKeyDomain": "goentri.com",
  "syncRedirectDomain": "api.goentri.com, goentri.com, entri.com, app.entri.com,app.goentri.com",
  "variableDescription":"hosttxt is the host for each TXT record,txt is the value for TXT record, hostcname is the host for the CNAME record and valuecname is the value to point CNAME",
  "sharedProviderName": true,
  "records": [
    {
      "type": "TXT",
      "host": "%hosttxt1%",
      "groupId": "a1",
      "data": "%txt1%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "%hostcname1%",
      "groupId": "a1",
      "pointsTo": "%valuecname1%",
      "ttl": 3600
    },
    {
      "type": "REDIR301",
      "target": "%target%",
      "groupId": "a1",
      "host": "@"
    },
    {
      "type": "TXT",
      "host": "%hosttxt2%",
      "groupId": "a2",
      "data": "%txt2%",
      "ttl": 3600
    },
    {
      "type": "TXT",
      "host": "%hosttxt3%",
      "groupId": "a3",
      "data": "%txt3%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "%hostcname3%",
      "groupId": "a3",
      "pointsTo": "%valuecname3%",
      "ttl": 3600
    }
  ]
}
