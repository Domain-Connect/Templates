{
  "providerId": "replit.com",
  "providerName": "Replit",
  "serviceId": "website",
  "serviceName": "Replit Custom Domains",
  "version": 1,
  "logoUrl": "https://replit.com/public/images/replit_logo_primary.svg",
  "description": "Setup a domain seamlessly on Replit",
  "syncBlock": false,
  "syncPubKeyDomain": "domainconnect.replit.com",
  "syncRedirectDomain": "replit.com",
  "records": [
    {
      "type": "A",
      "groupId": "apex",
      "host": "@",
      "pointsTo": "%ip%",
      "ttl": 3600
    },
    {
      "type": "TXT",
      "groupId": "apex-verification",
      "host": "@",
      "data": "replit-verify=%apex-verification%",
      "ttl": 3600
    },
    {
      "type": "A",
      "groupId": "subdomain",
      "host": "%subdomain%",
      "pointsTo": "%ip%",
      "ttl": 3600
    },
    {
      "type": "TXT",
      "groupId": "subdomain-verification",
      "host": "%subdomain%",
      "data": "replit-verify=%subdomain-verification%",
      "ttl": 3600
    }
  ]
}
