{
  "providerId": "verkh.io",
  "providerName": "Verkh",
  "serviceId": "spf",
  "serviceName": "SPF Email Authentication",
  "description": "Configure SPF record to authorize email senders for your domain",
  "variableDescription": "spfRecord: Complete SPF record value (e.g. v=spf1 include:_spf.google.com ~all)",
  "syncPubKeyDomain": "_domainconnect.verkh.io",
  "syncRedirectDomain": "verkh.io",
  "syncBlock": false,
  "shared": false,
  "multiInstance": false,
  "version": 1,
  "logoUrl": "https://verkh.io/logo.svg",
  "hostRequired": false,
  "warnPhishing": false,
  "records": [
    {
      "type": "SPFM",
      "host": "@",
      "spfRules": "%spfRecord%",
      "ttl": 3600
    }
  ]
}
