{
  "providerId": "w7sfw.com",
  "providerName": "W7SFW",
  "serviceId": "subdomain-setup",
  "serviceName": "W7SFW | WordPress Firewall",
  "version": 2,
  "logoUrl": "https://w7sfw.com/logo.svg",
  "description": "Configures DNS verification and routing for W7SFW WordPress Firewall",
  "variableDescription": "txtValue: domain verification token, subdomain: subdomain to protect, cnameTarget: proxy hostname prefix, ip: IPv4 address for apex routing",
  "syncPubKeyDomain": "domainconnect.w7sfw.com",
  "syncRedirectDomain": "w7sfw.com",
  "records": [
    {
      "type": "TXT",
      "groupId": "apex-verification",
      "host": "_w7sfw",
      "data": "w7sfw-domain-verify=%txtValue%",
      "ttl": 600
    },
    {
      "type": "TXT",
      "groupId": "subdomain-verification",
      "host": "_w7sfw",
      "data": "w7sfw-domain-verify=%txtValue%",
      "ttl": 600
    },
    {
      "type": "CNAME",
      "groupId": "subdomain",
      "host": "%subdomain%",
      "pointsTo": "%cnameTarget%",
      "ttl": 600
    },
    {
      "type": "CNAME",
      "groupId": "apex-cname",
      "host": "@",
      "pointsTo": "%cnameTarget%",
      "ttl": 600
    },
    {
      "type": "A",
      "groupId": "apex-a",
      "host": "@",
      "pointsTo": "%ip%",
      "ttl": 600
    }
  ]
}