{
  "providerId": "growsurf.com",
  "providerName": "GrowSurf",
  "serviceId": "email-whitelabel",
  "serviceName": "Email Whitelabel",
  "version": 1,
  "logoUrl": "https://app.growsurf.com/assets/img/logo/iconmark-blue.png",
  "description": "Configures DNS records for GrowSurf email whitelabel.",
  "variableDescription": "campaignId: GrowSurf campaign URL ID; dkimSelector: DKIM selector generated by GrowSurf; dkimValue: DKIM TXT value generated by GrowSurf; returnPathTarget: Return-path CNAME target generated by GrowSurf.",
  "syncBlock": false,
  "syncPubKeyDomain": "growsurf.com",
  "syncRedirectDomain": "app.growsurf.com,growsurf.com,app-staging.growsurf.com",
  "hostRequired": true,
  "records": [
    {
      "type": "TXT",
      "host": "@",
      "data": "growsurf=%campaignId%",
      "ttl": 3600
    },
    {
      "type": "TXT",
      "host": "%dkimSelector%._domainkey",
      "data": "%dkimValue%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "pm-bounces",
      "pointsTo": "%returnPathTarget%",
      "ttl": 3600
    }
  ]
}