{
  "providerId": "goentri.com",
  "providerName": "Entri",
  "serviceId": "fourthwall",
  "serviceName": "Fourthwall",
  "version": 3,
  "logoUrl": "https://cdn.goentri.com/logo.svg",
  "description": "Allows user to easily set up domain using Entri",
  "syncPubKeyDomain": "goentri.com",
  "syncRedirectDomain": "api.goentri.com, goentri.com",
  "variableDescription": "ip is the website server ip, fourthwallHost is the fourthwall host name to be added",
  "sharedProviderName": true,
  "records": [
    {
      "type": "A",
      "host": "%fourthwallHost%",
      "pointsTo": "%ip%",
      "ttl": 3600
    },
    {
      "type": "CNAME",
      "host": "%redirectValue%",
      "pointsTo": "%fourthwallHost%",
      "ttl": 3600
    }
  ]
}
