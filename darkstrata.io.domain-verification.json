{
  "providerId": "darkstrata.io",
  "providerName": "DarkStrata",
  "serviceId": "domain-verification",
  "serviceName": "Domain Verification",
  "version": 1,
  "logoUrl": "https://cdn.darkstrata.io/assets/logos/darkstrata_square_256.png",
