{
  "providerId": "builtbymagellan.com",
  "providerName": "Built by Magellan",
  "serviceId": "websitehosting",
  "serviceName": "Hébergement Site Web IA",
  "version": 1,
  "logoUrl": "https://builtbymagellan.com/logo.svg",
