{  
   "providerId":"canva.com",
   "providerName":"Canva",
   "serviceId":"website",
   "serviceName":"Canva Websites",
   "syncPubKeyDomain":"domainconnect.canva.com",
   "version":3,
   "logoUrl":"https://static.canva.com/static/images/canva_logo.svg",
   "description":"Enables a domain to work with Canva",
   "variableDescription":"verify-name and verify-value are used for the name and value of a verification record",
   "records":[
      {
         "groupId":"hosting",
         "type":"A",
         "host":"@",
         "pointsTo":"103.169.142.0",
         "ttl":300
      },
      {
         "groupId":"hosting",
         "type":"A",
         "host":"www",
         "pointsTo":"103.169.142.0",
         "ttl":300
      },
      {
         "groupId":"verificationTxt",
         "type":"TXT",
         "host":"%verify-name%",
         "data":"%verify-value%",
         "ttl":300
      },
      {
         "groupId":"verificationCname",
         "type":"CNAME",
         "host":"%verify-name%",
         "pointsTo":"%verify-value%",
         "ttl":300
      }
   ]
}
