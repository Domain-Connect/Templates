    {
      "pointsTo": "%v1%",
      "type": "A",
      "host": "admin"
    },
    {
      "pointsTo": "%v1%",
      "type": "A",
      "host": "mail"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "www"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "cpanel"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "webdisk"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "whm"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "www.admin"
    },
    {
      "pointsTo": "@",
      "type": "CNAME",
      "host": "webdisk.admin"
    }
  ]
}
